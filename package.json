{
  "name": "weaveworld",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "test": "vitest",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006 --no-open",
    "build-storybook": "storybook build",
    "aos-d": "func() { aos ${1} --wallet ./fixtures/deploy_jwk.json ; }; func",
    "aos-d-load": "func() { aos ${1} --wallet ./fixtures/deploy_jwk.json --load ${2} ; }; func",
    "aos-d-load-verse": "func() { npm run aos-d-load ${1} process/blueprint/Verse.lua ; }; func",
    "aos-d-load-chat": "func() { npm run aos-d-load ${1} process/blueprint/Chat.lua ; }; func",
    "aos-d-load-specific": "func() { npm run aos-d-load ${1} process/${2}_${1}.lua ; }; func",
    "deploy-all-verse": "concurrently \"npm:aos-d-load-verse Singularity\" \"npm:aos-d-load-verse Universe\" \"npm:aos-d-load-verse WeaveWorld\" \"npm:aos-d-load-verse OriginIsland\" \"npm:aos-d-load-verse LlamaFed\"",
    "deploy-all-chat": "concurrently \"npm:aos-d-load-chat WeaveWorld\" \"npm:aos-d-load-chat OriginIsland\" \"npm:aos-d-load-chat LlamaFed\"",
    "deploy-all-specific": "concurrently \"npm:aos-d-load-specific Singularity 0\" \"npm:aos-d-load-specific Universe 1\" \"npm:aos-d-load-specific WeaveWorld 2\" \"npm:aos-d-load-specific OriginIsland 3\" \"npm:aos-d-load-specific LlamaFed 4\" \"npm:aos-d-load-specific LlamaAssistant 5\"",
    "deploy-all": "concurrently \"npm:deploy-all-verse\" \"npm:deploy-all-chat\" \"npm:deploy-all-specific\""
  },
  "dependencies": {
    "@othent/kms": "^1.0.12",
    "@permaweb/aoconnect": "^0.0.54",
    "@radix-ui/react-slot": "^1.0.2",
    "@tanstack/react-query": "^5.39.0",
    "arweave": "^1.15.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "lucide-react": "^0.379.0",
    "phaser": "^3.80.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "warp-arbundles": "^1.0.4",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.4.0",
    "@storybook/addon-essentials": "^8.1.3",
    "@storybook/addon-interactions": "^8.1.3",
    "@storybook/addon-links": "^8.1.3",
    "@storybook/addon-onboarding": "^8.1.3",
    "@storybook/blocks": "^8.1.3",
    "@storybook/react": "^8.1.3",
    "@storybook/react-vite": "^8.1.3",
    "@storybook/test": "^8.1.3",
    "@types/node": "^20.12.12",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "eslint-plugin-storybook": "^0.8.0",
    "postcss": "^8.4.38",
    "storybook": "^8.1.3",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vitest": "^1.6.0"
  }
}
